{
    "name": "frilling3",
    "version": "v3.5.0",
    "description": "Grav theme",
    "main": "js/app.js",
    "scripts": {
        "start": "npm-run-all lint build",
        "build": "npm-run-all --parallel css js:dev",
        "dist": "npm-run-all --parallel css js:dist",
        "css": "npm-run-all --parallel css:app css:bootstrap",
        "css:app": "node tasks/css-app",
        "css:bootstrap": "node tasks/css-bootstrap",
        "js:dev": "node tasks/js-dev",
        "js:dist": "node tasks/js-dist",
        "lint": "eslint js/"
    },
    "namespace": {
        "module": "frilling"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/frilling.com.git"
    },
    "author": "FelixRilling",
    "license": "MIT",
    "homepage": "https://f-rilling.com",
    "dependencies": {
        "bootstrap": "^4.0.0-alpha.6"
    },
    "devDependencies": {
        "babel-core": "7.0.0-alpha.7",
        "babel-preset-es2015": "7.0.0-alpha.7",
        "eslint": "^3.19.0",
        "node-sass": "^4.5.3",
        "npm-run-all": "^4.0.2",
        "pretty": "^2.0.0",
        "pug": "^2.0.0-rc.1",
        "rollup": "^0.41.6",
        "rollup-plugin-commonjs": "^8.0.2",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-replace": "^1.1.1",
        "uglify-es": "^3.0.4"
    }
}
