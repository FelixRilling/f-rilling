{
    "name": "f-rilling.com",
    "version": "v3.8.0",
    "description": "f-rilling.com custom theme",
    "main": "js/app.js",
    "scripts": {
        "start": "npm-run-all lint build",
        "build": "npm-run-all --parallel css:dev js:dev",
        "dist": "npm-run-all --parallel css:dist js:dist",
        "css:dev": "node-sass scss/app.scss -o ../css/ --output-style expanded",
        "css:dist":
            "node-sass scss/app.scss -o ../css/ --output-style compressed",
        "js:dev":
            "cross-env NODE_ENV=development webpack --progress --hide-modules",
        "js:dist":
            "cross-env NODE_ENV=production webpack --progress --hide-modules",
        "lint": "eslint js/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/frilling.com.git"
    },
    "author": "FelixRilling",
    "license": "MIT",
    "homepage": "https://f-rilling.com",
    "dependencies": {
        "bootstrap": "^4.0.0-beta.2",
        "lightdash": "^3.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.34",
        "@babel/preset-env": "^7.0.0-beta.34",
        "babel-loader": "^8.0.0-beta.0",
        "cross-env": "^5.1.1",
        "eslint": "^4.12.1",
        "node-sass": "^4.7.2",
        "npm-run-all": "^4.1.2",
        "prettier": "^1.9.1",
        "rimraf": "^2.6.2",
        "uglify-es": "^3.2.1",
        "uglifyjs-webpack-plugin": "^1.1.2",
        "webpack": "^3.10.0"
    }
}
