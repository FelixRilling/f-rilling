{
    "name": "rilling.dev_theme",
    "version": "4.0.0",
    "private": true,
    "description": "rilling.dev custom theme",
    "scripts": {
        "start": "npm run dist",
        "dist": "npm-run-all clean lint build:dist",
        "build": "npm-run-all --parallel css js",
        "build:dist": "npm-run-all --parallel css:dist js:dist",
        "watch": "npm-run-all --parallel css:watch js:watch",
        "clean": "rimraf ./source/css/ ./source/js/",
        "css": "node-sass src/scss/main.scss -o ./source/css/ --output-style expanded",
        "css:watch": "node-sass src/scss/main.scss -o ./source/css/ --output-style expanded --watch",
        "css:dist": "node-sass src/scss/main.scss -o ./source/css/ --output-style compressed",
        "js": "webpack -d",
        "js:watch": "webpack -d --watch",
        "js:dist": "webpack -p",
        "lint": "eslint ./src/ts/ --ext .ts",
        "fix": "eslint ./src/ts/ --ext .ts --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/rilling.dev_theme.git"
    },
    "author": "FelixRilling",
    "license": "MIT",
    "homepage": "https://rilling.dev",
    "dependencies": {
        "bootstrap": "^4.4.1",
        "lightdash": "^11.1.2"
    },
    "devDependencies": {
        "@types/node": "^13.7.1",
        "@typescript-eslint/eslint-plugin": "^2.19.2",
        "@typescript-eslint/parser": "^2.19.2",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "node-sass": "^4.13.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.2",
        "terser": "^4.6.3",
        "terser-webpack-plugin": "^2.3.5",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.5",
        "webpack": "^4.41.6",
        "webpack-cli": "^3.3.11"
    }
}
